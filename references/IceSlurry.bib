@article{kousksou_crystallisation_2012,
	title = {Crystallisation kinetics with nucleation phenomenon: {Ice} slurry system},
	volume = {35},
	issn = {0140-7007},
	shorttitle = {Crystallisation kinetics with nucleation phenomenon},
	url = {//www.sciencedirect.com/science/article/pii/S0140700712001429},
	doi = {10.1016/j.ijrefrig.2012.05.018},
	abstract = {In this paper a physical model is built to investigate the non-isothermal freezing kinetic in ice slurry systems. The global Nakamura-type kinetics is adopted to describe the nucleation rate during the freezing process. A genetic algorithm is used to determine the nucleation probability of ice slurry. The finite difference method is utilized to solve the heat transfer problem, and the source term method is employed to handle the absorbed latent heat. An experimental example of a cylindrical cell is used to test this model. From a comparison with the experimental result, the computed cooling curve is found to be very close to the experimental one.},
	number = {7},
