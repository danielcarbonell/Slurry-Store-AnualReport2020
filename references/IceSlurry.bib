@article{wang_effects_2015,
	title = {Effects of surface characteristic on frosting and defrosting behaviors of fin-tube heat exchangers},
	volume = {75},
	issn = {1359-4311},
	url = {http://www.sciencedirect.com/science/article/pii/S1359431114009727},
	doi = {10.1016/j.applthermaleng.2014.10.090},
	abstract = {Surface characteristic is one of the most important factors that affect the frosting and defrosting behaviors of a fin-tube heat exchanger in an air source heat pump. In this paper, three types of fin-tube heat exchangers with different surface characteristics were prepared and the effects of the surface characteristic on the frosting and defrosting behaviors of the heat exchangers were investigated experimentally. It was found that the surface characteristic had an obvious effect on the frost layer growth. The frost thickness and mass of the superhydrophobic heat exchanger were 17.1\% and 28.8\% less than those of the bare heat exchanger. What is more, the superhydrophobic heat exchanger showed the lowest air-side pressure drop and the best heat transfer during the frosting process. The effects of the surface characteristic on the energy consumption and time for frost melting were significant. When the frost layer melted completely, the retained water formed a thin water film on the surfaces of the hydrophilic fins while only some small spherical droplets stayed on the surfaces of the superhydrophobic fins. The least retained water on the superhydrophobic heat exchanger can reduce the energy consumption for evaporation and improve the defrosting efficiency.},
	urldate = {2017-05-12},
	journal = {Applied Thermal Engineering},
	author = {Wang, Feng and Liang, Caihua and Yang, Mingtao and Fan, Chen and Zhang, Xiaosong},
	month = jan,
	year = {2015},
	keywords = {Defrosting, Fin-tube heat exchanger, Frosting, Molten water retention, Surface characteristic},
	pages = {1126--1132},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\dcarbone\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1sshl2r7.default\\zotero\\storage\\MI5PVR3P\\Wang et al. - 2015 - Effects of surface characteristic on frosting and .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\dcarbone\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1sshl2r7.default\\zotero\\storage\\DV7XPJP9\\S1359431114009727.html:text/html}
