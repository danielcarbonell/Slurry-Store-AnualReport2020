@article{zhang_review_2015,
	title = {A review of the recent advances in superhydrophobic surfaces and the emerging energy-related applications},
	volume = {82},
	issn = {0360-5442},
	url = {http://www.sciencedirect.com/science/article/pii/S0360544215000857},
	doi = {10.1016/j.energy.2015.01.061},
	abstract = {A new kind of functional surfaces with particular characteristics, i.e., superhydrophobic surfaces, has recently been developed and applied in many fields, such as airplane, wind turbine, electric power line, photovoltaic cell, heat exchanges, ice slurry generator, and so on. The freezing delay and ice-accumulation avoiding on the surfaces are important to keep stable working condition for these devices. The frictional pressure loss of flow through the tubes or channels with superhydrophobic surfaces is much smaller than that through those without superhydrophobic surfaces. Both the boiling and condensation heat transfer performances on superhydrophobic surfaces can be enhanced. The superhydrophobic surfaces have potential applications and are worthy further investigations. We provide here a review of the fabrications, characterization and the emerging energy-related applications of superhydrophobic surfaces on the basis of the recent progresses of the research and development in this field. The fabrication of superhydrophobic surface, in particular a recently developed SLIPS (slippery liquid-infused porous surface), is summarized. The focuses are placed on the particular characteristics of superhydrophobic surfaces and their applications in energy-related fields. The further research topics are also clarified to promote the future applications.},
	urldate = {2017-05-11},
	journal = {Energy},
	author = {Zhang, P. and Lv, F. Y.},
	month = mar,
	year = {2015},
	keywords = {Friction reduction, heat transfer, Ice adhesion strength, SLIPS (slippery liquid-infused porous surface), Superhydrophobic surface},
	pages = {1068--1087},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\dcarbone\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1sshl2r7.default\\zotero\\storage\\5B36ZGVR\\Zhang and Lv - 2015 - A review of the recent advances in superhydrophobi.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\dcarbone\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1sshl2r7.default\\zotero\\storage\\NEMRVU8W\\S0360544215000857.html:text/html}
}
@article{moallem_experimental_2012,
	title = {Experimental measurements of the surface coating and water retention effects on frosting performance of microchannel heat exchangers for heat pump systems},
	volume = {39},
	issn = {0894-1777},
	url = {http://www.sciencedirect.com/science/article/pii/S0894177712000337},
	doi = {10.1016/j.expthermflusci.2012.01.022},
	abstract = {Frost formation on louvered fin microchannel heat exchangers was experimentally investigated in this paper with the aim of studying the effect of hydrophilic and hydrophobic surface coatings and of water retention on their frosting performance and on their frost growth rates. Frost mass and thickness growth rates, corresponding coil heat transfer rates, capacity degradation, and air-side pressure drop for five microchannel coils with different surface coatings were measured and discussed. The experimental data showed that the hydrophilic and hydrophobic surface coated coils considered in the present work accumulated frost on the heat transfer surfaces with visible difference in the type, appearance, and patterns of the frost. The duration of the frosting cycle, the heat transfer rate, and air-side pressure drop were similar to the ones of a louvered aluminum fin microchannel heat exchanger with uncoated surfaces. For the operating conditions tested in this work, hydrophobic and hydrophilic coatings on microchannel coils affected the heat transfer capacity in frosting conditions by up to 15\%.},
	urldate = {2017-05-12},
	journal = {Experimental Thermal and Fluid Science},
	author = {Moallem, Ehsan and Cremaschi, Lorenzo and Fisher, Daniel E. and Padhmanabhan, Sankar},
	month = may,
	year = {2012},
	keywords = {Capacity reduction, Frost, Heat pump, Microchannel, Pressure drop, Surface coating},
	pages = {176--188},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\dcarbone\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1sshl2r7.default\\zotero\\storage\\GQ927TGB\\Moallem et al. - 2012 - Experimental measurements of the surface coating a.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\dcarbone\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1sshl2r7.default\\zotero\\storage\\5KDMZBWW\\S0894177712000337.html:text/html}
